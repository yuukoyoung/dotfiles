{
  // 3rd Party Extensions Settings Start
  // Macros
  "macros": {
    "collapseAndClose": [
      "workbench.files.action.collapseExplorerFolders",
      "workbench.action.closeAllEditors"
    ],
    "cursorFiveLineUp": [
      "cursorUp",
      "cursorUp",
      "cursorUp",
      "cursorUp",
      "cursorUp"
    ],
    "cursorFiveLineDown": [
      "cursorDown",
      "cursorDown",
      "cursorDown",
      "cursorDown",
      "cursorDown"
    ]
  },
  // Gitlens
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  // Prettier
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.printWidth": 80,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.quoteProps": "as-needed",
  "prettier.jsxSingleQuote": false,
  "prettier.trailingComma": "all",
  "prettier.bracketSpacing": true,
  "prettier.bracketSameLine": true,
  "prettier.arrowParens": "always",
  "prettier.requirePragma": false,
  "prettier.insertPragma": false,
  "prettier.proseWrap": "preserve",
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.vueIndentScriptAndStyle": false,
  "prettier.endOfLine": "lf",
  "prettier.embeddedLanguageFormatting": "auto",
  // Eslint & Stylelint
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "jsonc",
    "json5",
    "vue"
  ],
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "jsonc",
    "json5",
    "vue"
  ],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "stylelint.validate": ["css", "less", "postcss"],
  // Path Intellisense
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  // Markdownlint
  "markdownlint.ignore": [".changeset/*.md"],
  "markdownlint.config": {
    "MD024": false,
    "MD025": false,
    "MD034": false,
    "MD040": false
  },
  // Vue
  "vue3snippets.enable-compile-vue-file-on-did-save-code": false,
  // Parameter Hints
  "parameterHints.hintingType": "variableOnly",
  // 3rd Party Extensions Settings End

  // Other Settings Start
  // Update
  "update.mode": "manual",
  // Settings,
  "workbench.settings.useSplitJSON": false,
  "workbench.settings.editor": "json",
  // Theme,
  "workbench.colorTheme": "Tiny Light",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.colorCustomizations": {
    "editorWhitespace.foreground": "#E4E4E4"
  },
  // Fonts
  "editor.fontFamily": "Fira Code, Consolas, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "Fira Code, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "terminal.integrated.fontSize": 17,
  "editor.lineHeight": 24.65, // fontSize * 1.45
  "editor.fontWeight": "600",
  "editor.letterSpacing": 0.5,
  // Cursor
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 3,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.multiCursorModifier": "ctrlCmd",
  // UI
  "breadcrumbs.enabled": true,
  "debug.toolBarLocation": "docked",
  "editor.accessibilitySupport": "off",
  "editor.codeLens": false,
  "editor.lineNumbers": "interval",
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "on"
  },
  "editor.quickSuggestionsDelay": 0,
  "editor.rulers": [80, 100, 120],
  "editor.snippetSuggestions": "inline",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.suggestSelection": "recentlyUsed",
  "editor.tabCompletion": "on",
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "window.dialogStyle": "custom",
  "window.restoreFullscreen": true,
  "window.titleBarStyle": "custom",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 5,
  "workbench.editor.tabCloseButton": "off",
  "workbench.list.smoothScrolling": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.expandMode": "singleClick",
  "workbench.tree.indent": 10,
  // AutoImports
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  // Tags
  "editor.linkedEditing": true,
  "html.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "typescript.autoClosingTags": true,
  // BracketPairs
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  // FileNesting
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.js": "$(capture).js.map, $(capture).*.js",
    "*.ts": "$(capture).js, $(capture).*.ts",
    "*.css": "$(capture).css.map, $(capture).*.css",
    "*.vue": "$(capture).*.js, $(capture).*.ts, $(capture).*.mdx",
    ".gitignore": ".gitattributes",
    "readme.md": "readme*, license*, changelog*",
    "package.json": ".npm*, package-lock.json, .yarn*, yarn*, .pnpm*, pnpm*, .editorconfig, .prettier*, prettier*, .eslint*, .stylelint*, stylelint*, .commitlint*, commitlint*, .czrc, .cz-config*, tsconfig.*, turbo.json, postcss*, tailwind*"
  },
  // Terminal
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.fontWeight": "300",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.tabs.enabled": true,
  // Code Style
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "files.eol": "\n",
  "files.encoding": "utf8",
  // Whitespace
  "editor.renderWhitespace": "boundary",
  "files.trimTrailingWhitespace": true,
  "editor.trimAutoWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "diffEditor.ignoreTrimWhitespace": false,
  // Formatting
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  // Git
  "git.autofetch": true,
  "git.autoStash": true,
  "git.untrackedChanges": "separate",
  // Emmet,
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  // Files
  "files.autoSave": "afterDelay",
  "files.enableTrash": true,
  "files.associations": {
    "LICENSE": "plaintext",
    "*.css": "css"
  },
  // Typescript
  "typescript.tsdk": "./node_modules/typescript/lib",
  // JSON
  "json.schemaDownload.enable": false,
  // Markdown
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  // Search
  "search.exclude": {
    "**/.git": true,
    "**/.github": true,
    "**/.DS_store": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/.pnpm": true,
    "**/pnpm-lock.yaml": true,
    "**/.pnpm-store": true,
    "**/.yarn": true,
    "**/yarn.lock": true,
    "**/dist": true,
    "**/*.log": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.vscode": true,
    "**/.changeset": true,
    "**/.turbo": true,
    "**/coverage": true,
    "**/storybook-static": true
  },
  // Other Settings End

  // Dynamic Settings Start
  "gitlens.views.branches.branches.layout": "list",
  "gitlens.advanced.messages": {
    "suppressCreatePullRequestPrompt": true
  },
  "workbench.activityBar.visible": false
  // Dynamic Settings End
}
